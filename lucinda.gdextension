[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://lucinda/target/debug/liblucinda.so"
linux.release.x86_64 =   "res://lucinda/target/release/liblucinda.so"
windows.debug.x86_64 =   "res://lucinda/target/debug/lucinda.dll"
windows.release.x86_64 = "res://lucinda/target/release/lucinda.dll"
macos.debug =            "res://lucinda/target/debug/liblucinda.dylib"
macos.release =          "res://lucinda/target/release/liblucinda.dylib"
macos.debug.arm64 =      "res://lucinda/target/debug/liblucinda.dylib"
macos.release.arm64 =    "res://lucinda/target/release/liblucinda.dylib"

